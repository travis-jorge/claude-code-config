{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash",
      "Read",
      "Glob",
      "Grep"
    ]
  },
  "model": "sonnet",
  "statusLine": {
    "type": "command",
    "command": "bash {{HOME}}/.claude/statusline.sh"
  },
  "enabledPlugins": {},
  "alwaysThinkingEnabled": false
}
